{
    "algorithm": "PPO",
    "env_id": "intersection-v0",
    "env_config": {
        "observation": {
            "type": "Kinematics",
            "vehicles_count": 15,
            "features": [
                "presence",
                "x",
                "y",
                "vx",
                "vy",
                "cos_h",
                "sin_h"
            ],
            "features_range": {
                "x": [
                    -100,
                    100
                ],
                "y": [
                    -100,
                    100
                ],
                "vx": [
                    -20,
                    20
                ],
                "vy": [
                    -20,
                    20
                ]
            },
            "absolute": true,
            "flatten": false,
            "observe_intentions": false
        },
        "action": {
            "type": "DiscreteMetaAction",
            "longitudinal": false,
            "lateral": true
        }
    },
    "n_envs": 32,
    "total_timesteps": 500000,
    "tuning_fraction": 0.5,
    "n_trials": 35,
    "search_space": {
        "learning_rate": {
            "method": "log_uniform",
            "low": 1e-5,
            "high": 1e-3
        },
        "n_steps": {
            "method": "int_uniform",
            "low": 512,
            "high": 4096
        },
        "batch_size": {
            "method": "categorical",
            "choices": [
                64,
                128,
                256
            ]
        },
        "n_epochs": {
            "method": "categorical",
            "choices": [
                5,
                10,
                20
            ]
        },
        "gamma": {
            "method": "uniform",
            "low": 0.95,
            "high": 0.999
        },
        "gae_lambda": {
            "method": "uniform",
            "low": 0.9,
            "high": 0.98
        },
        "clip_range": {
            "method": "uniform",
            "low": 0.1,
            "high": 0.4
        },
        "ent_coef": {
            "method": "log_uniform",
            "low": 1e-5,
            "high": 1e-2
        }
    }
}